{
    "background": {
        "scripts": ["js/sniff.js"]
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon19.png",
            "38": "img/icon38.png"
        },
        "default_title": "SAS Sniffer"
    },
    "description": "Retrieve all HTTP/HTTPs URLs from your browser.",
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "manifest_version": 2,
    "name": "SAS Sniffer",
    "offline_enabled": true,
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "version": "1.0"
}
